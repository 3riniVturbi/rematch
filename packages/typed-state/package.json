{
	"name": "@rematch/typed-state",
	"version": "1.0.0",
	"description": "Rematch plugin for typechecking state",
	"homepage": "https://rematch.github.io/rematch",
	"main": "dist/index.js",
	"module": "dist/typed-state.esm.js",
	"typings": "src/typings.d.ts",
	"scripts": {
		"start": "tsdx watch",
		"build": "tsdx build --format cjs,esm,umd",
		"test": "tsdx test",
		"lint": "eslint . --ext .js,.ts,.json",
		"clean": "rimraf dist .rts2_cache_cjs .rts2_cache_esm .rts2_cache_umd"
	},
	"files": [
		"dist",
		"src"
	],
	"dependencies": {},
	"devDependencies": {
		"@rematch/core": "^2.0.0",
		"husky": "^3.1.0",
		"prop-types": "^15.7.2"
	},
	"peerDependencies": {
		"@rematch/core": ">=2.0.0",
		"prop-types": "^15.6.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint"
		}
	},
	"author": "Konstantin Lebedev <koss.lebedev@gmail.com> (https://github.com/koss-lebedev)",
	"keywords": [
		"@rematch",
		"rematch",
		"plugin",
		"typings",
		"typecheck"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rematch/rematch.git"
	}
}
